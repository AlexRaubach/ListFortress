<div class='container'>
<h1>Seasons#index</h1>
<p>Find me in app/views/seasons/index.html.erb</p>
<table  class="table table-striped">
<% @seasons.each do |season| %>
  <tr>
    <td><%= season.season_number%></td>
    <td><%= link_to  season.name, season_path(season) %><td>
  </tr>
<% end %>
</table>
</div>